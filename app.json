{
  "expo": {
    "name": "GlamGuide",
    "slug": "glam_guide",
    "icon": "./assets/images/icon.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/images/icon.png",
        "light": "./assets/images/icon.png",
        "tinted": "./assets/images/icon.png"
      },
      "bundleIdentifier": "n1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "monochromeImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nbarnhouse.glam_guide"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-localization",
      "expo-build-properties",
      [
      "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/inter/Aboreto-Regular.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ef852fa0-d227-4e3f-aa77-4b5b85304d14"
      }
    }
  }
}
